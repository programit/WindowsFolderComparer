<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="FolderComparer.CustomTb"
	x:Name="UserControl"
	d:DesignWidth="320" d:DesignHeight="23">

	<Grid x:Name="LayoutRoot">
		<TextBox x:Name="tbLeft" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Background="#FF676767" Foreground="#FFD1D1D1" BorderBrush="#FF434343" RenderTransformOrigin="0,0" Style="{DynamicResource TextBoxStyle1}"/>
	</Grid>
</UserControl>